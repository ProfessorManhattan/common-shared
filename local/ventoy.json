{
  "control": [
        { "VTOY_DEFAULT_SEARCH_ROOT": "/iso" },
        { "VTOY_MENU_TIMEOUT": "30" },
        { "VTOY_DEFAULT_IMAGE": "/iso/qubes-custom.iso" }
    ],
    "theme": {
        "file": [
          "/ventoy/theme/distro-grub-themes/customize/ventoy/theme.txt"
        ],
        "default_file": 1,
        "gfxmode": "1920x1080",
        "display_mode": "GUI",
        "serial_param": "--unit=0 --speed=9600",
        "ventoy_left": "5%",
        "ventoy_top": "95%",
        "ventoy_color": "#0000ff",
        "fonts": [
            "/ventoy/theme/ventoy/terminus-12.pf2",
            "/ventoy/theme/ventoy/terminus-14.pf2",
            "/ventoy/theme/ventoy/terminus-16.pf2",
            "/ventoy/theme/ventoy/terminus-18.pf2"
        ]
    },
    "auto_install": [
      {
        "autosel": 1,
        "image": "/iso/qubes-custom.iso",
        "template": [
          "/ventoy/scripts/qubes/kickstart-desktop-custom.cfg"
        ],
        "timeout": 30
      },
        {
          "parent": "/iso/fedora",
          "template": [
            "/ventoy/scripts/fedora/kickstart-server.cfg",
            "/ventoy/scripts/fedora/kickstart-workstation-minimal.cfg",
            "/ventoy/scripts/fedora/kickstart-workstation-full.cfg"
          ]
        },
        {
          "parent": "/iso/qubes",
          "template": [
            "/ventoy/scripts/qubes/kickstart-desktop-minimal.cfg",
            "/ventoy/scripts/qubes/kickstart-desktop-full.cfg"
          ]
        },
        {
          "parent": "/iso/ubuntu",
          "template": [
            "/ventoy/scripts/ubuntu/cloud-init-server",
            "/ventoy/scripts/ubuntu/cloud-init-desktop-minimal",
            "/ventoy/scripts/ubuntu/cloud-init-desktop-full"
          ]
        },
        {
            "parent": "/iso/windows",
            "template": [
                "/ventoy/scripts/windows-11-minimal.xml",
                "/ventoy/scripts/windows-11-full.xml"
            ]
        }
    ],
    "injection": [
        {
            "parent": "/iso/fedora",
            "archive": "/assets/playbooks.tar.gz"
        },
        {
            "parent": "/iso/qubes",
            "archive": "/assets/playbooks.tar.gz"
        },
      {
            "parent": "/iso/ubuntu",
            "archive": "/assets/playbooks.tar.gz"
        },
        {
            "parent": "/iso/windows",
            "archive": "/assets/playbooks.zip"
        }
    ],
    "menu_class": [
        {
            "key": "archlinux",
            "class": "arch"
        },
        {
          "key": "clonezilla",
          "class": "clonezilla"
        },
        {
            "key": "elementary",
            "class": "elementary"
        },
        {
            "key": "kali",
            "class": "kali"
        },
        {
            "key": "kaspersky",
            "class": "krd"
        },
        {
            "key": "ubuntu",
            "class": "ubuntu"
        },
        {
            "key": "zorin",
            "class": "zorin"
        },
        {
            "parent": "/iso/archlinux",
            "class": "arch"
        },
        {
            "parent": "/iso/fedora",
            "class": "fedora"
        },
        {
            "parent": "/iso/qubes",
            "class": "qubes"
        },
        {
            "parent": "/iso/ubuntu",
            "class": "ubuntu"
        },
        {
            "parent": "/iso/windows",
            "class": "windows"
        }
    ],
    "menu_alias": [
        {
          "image": "/iso/fedora/fedora-server-36.iso",
          "alias": "Install Fedora 36"
        },
        {
          "image": "/iso/qubes/qubes-4.1.0.iso",
          "alias": "Install Qubes 4.1"
        },
        {
            "image": "/iso/ubuntu/ubuntu-server-22.04.iso",
            "alias": "Install Ubuntu 22.04 (Jammy Jellyfish)"
        },
        {
            "dir": "/iso/windows/windows-11.iso",
            "alias": "Install Windows 11"
        },
        {
          "image": "/iso/live/archlinux.iso",
          "alias": "Run ArchLinux (Live)"
        },
        {
          "image": "/iso/live/clonezilla.iso",
          "alias": "Run CloneZilla (Live)"
        },
        {
          "image": "/iso/live/elementary.iso",
          "alias": "Run ElementaryOS (Live)"
        },
        {
          "image": "/iso/live/kali.iso",
          "alias": "Run Kali (Live)"
        },
        {
          "image": "/iso/live/kaspersky-rescue-disk.iso",
          "alias": "Run Kaspersky Rescue Disk (Live)"
        },
        {
          "image": "/iso/live/ubuntu.iso",
          "alias": "Run Ubuntu 22.04 (Jammy Jellyfish) (Live)"
        },
        {
          "image": "/iso/live/zorin.iso",
          "alias": "Run ZorinOS (Live)"
        }
    ],
    "persistence": [
      {
        "image": "/iso/live/archlinux.iso",
        "backend": [
          "/persistence/archlinux-live.dat",
          "/persistence/archlinux-live-alternate.dat"
        ]
      },
      {
            "image": "/iso/live/elementary.iso",
            "backend": [
                "/persistence/elementary-live.dat",
                "/persistence/elementary-live-alternate.dat"
            ]
        },
        {
            "image": "/iso/live/kali.iso",
            "backend": [
                "/persistence/kali-live.dat",
                "/persistence/kali-live-alternate.dat"
            ]
        },
        {
          "image": "/iso/live/kaspersky-rescue-disk.iso",
          "backend": [
            "/persistence/krd.dat",
            "/persistence/krd-alternate.dat"
          ]
        },
        {
            "image": "/iso/live/ubuntu.iso",
            "backend": [
              "/persistence/ubuntu-live.dat",
              "/persistence/ubuntu-live-alternate.dat"
            ]
        },
        {
          "image": "/iso/live/zorin.iso",
          "backend": [
            "/persistence/zorin-live.dat",
            "/persistence/zorin-live-alternate.dat"
          ]
        }
    ]
}
